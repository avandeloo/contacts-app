<%= form_tag "/contacts", method: :post do %>
  <div>
    <%= label_tag :first_name, nil, placeholder: "First Name" %>
    <%= text_field_tag :first_name%>
  </div>
  <div>
    <%= label_tag :image, nil, placeholder: "Last Name" %>
    <%= text_field_tag :last_name%>
  </div>
  <div>
    <%= label_tag :email, nil, placeholder: "Email Url" %>
    <%= text_field_tag :email%>
  </div>
  <div>
    <%= label_tag :phone_number, nil, placeholder: "phone_number" %>
    <%= text_field_tag :phone_number%>
  </div>
  <%= submit_tag "Submit" %>

<% end %>
